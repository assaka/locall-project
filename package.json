{
  "name": "locall",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --watchAll=false",
    "test:unit": "jest --testPathIgnorePatterns=e2e",
    "test:integration": "jest --testMatch='**/*.integration.test.{js,ts,tsx}'",
    "test:e2e": "playwright test",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:ui": "playwright test --ui",
    "deploy:staging": "NODE_ENV=staging node scripts/deploy.js",
    "deploy:production": "NODE_ENV=production node scripts/deploy.js",
    "health-check": "curl -f http://localhost:3000/api/health || exit 1",
    "db:migrate": "echo 'Run database migrations manually'",
    "security-check": "npm audit --audit-level moderate",
    "bundle-analyze": "ANALYZE=true npm run build"
  },
  "dependencies": {
    "@emotion/css": "^11.13.5",
    "@emotion/react": "^11.14.0",
    "@emotion/serialize": "^1.3.3",
    "@emotion/styled": "^11.14.1",
    "@mui/icons-material": "^6.4.12",
    "@mui/material": "^6.4.12",
    "@mui/styled-engine": "^7.2.0",
    "@mui/system": "^6.4.12",
    "@prisma/client": "^6.10.1",
    "@sendgrid/mail": "^8.1.5",
    "@supabase/auth-ui-react": "^0.4.7",
    "@supabase/auth-ui-shared": "^0.1.8",
    "@supabase/supabase-js": "^2.50.0",
    "@types/react": "^18.3.23",
    "@types/react-dom": "^18.3.7",
    "@vonage/server-sdk": "^3.21.2",
    "axios": "^1.10.0",
    "chalk": "^5.4.1",
    "jsonwebtoken": "^9.0.2",
    "next": "15.3.4",
    "nodemailer": "^7.0.3",
    "prisma": "^6.10.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "recharts": "^3.0.0",
    "resend": "^4.6.0",
    "stripe": "^18.2.1",
    "zod": "^3.22.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@playwright/test": "^1.40.0",
    "@testing-library/jest-dom": "^6.1.4",
    "@testing-library/react": "^14.1.2",
    "@testing-library/user-event": "^14.5.1",
    "@types/axios": "^0.14.0",
    "@types/jest": "^29.5.8",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^20",
    "@types/nodemailer": "^6.4.17",
    "@types/supertest": "^6.0.0",
    "eslint": "^9",
    "eslint-config-next": "15.3.4",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-mock-extended": "^3.0.5",
    "msw": "^2.0.0",
    "supertest": "^6.3.3",
    "typescript": "^5"
  }
}
